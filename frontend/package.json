{
  "name": "tim-frontend",
  "version": "1.0.0",
  "description": "Mobile-first frontend for TiM time tracking application",
  "private": true,
  "type": "module",
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "@tanstack/react-query": "^5.84.1",
